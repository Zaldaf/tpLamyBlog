{% extends 'base.html.twig' %}

{% block title %}Liste des Articles{% endblock %}

{% block body %}
    <ul>
        {% for article in articles %}

            <li><a href="{{ path('app_contenue_slug', {slug : article.slug}) }}"> {{ article.titre }}  </a>{{ article.creatAt | date('m/d/Y') }} </li>

        {% endfor %}
    </ul>
{% endblock %}
