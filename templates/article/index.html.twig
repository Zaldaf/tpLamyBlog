{% extends 'base.html.twig' %}

{% block title %}Liste des Articles{% endblock %}

{% block body %}
    <ul>
        {% for article in articles %}

            <li><a href="{{ path('app_contenue_id', {id : article.id}) }}"> {{ article.titre }} {{ article.creatAt | date('m/d/Y') }} </a> </li>

        {% endfor %}
    </ul>
{% endblock %}
